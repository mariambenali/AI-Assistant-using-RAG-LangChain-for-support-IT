name: CI -FASTAPI RAG
on:
    push:
        branches:
            -develop
    pull request:

jobs:
    build-test:
        runs-on: ubuntu-latest

        steps:
            -name: checkout code
            usues: actions/checkout@v4

            -name: set up python
            uses: actions/checkout@v5
            with:
                python-version: "3.11"
            
            -name: install dependecies
            run:| 
                python -m pip install --upgrade pip
                pip install -r requirements.txt

            name: run tests
            run:|
                pytest


            
